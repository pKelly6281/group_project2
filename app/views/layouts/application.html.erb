<!DOCTYPE html>
<html>
<head>
  <title><%= yield(:title) %> | Group Project Two</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <%=render 'layouts/shim'%>
</head>
<body>
 	<%= render 'layouts/header'%>
 	<div class= "container">

    <h1><%= yield(:header)%></h1>
    <div id= "wc">
      <%= yield%>

    </div>
		  
		<div id="images">
 			<%= link_to image_tag(yield(:imageName), alt: yield(:imageAlt)),
            yield(:imageLink)%>
 		</div>
		<%= render 'layouts/footer'%>
 	</div>

	
</body>
</html>
